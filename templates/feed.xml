<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <id>${feedUrl}</id>
  <title>${config.site.name}</title>
  <updated>${config.rfc3339Format(new Date())}</updated>
  <link rel="self" href="${feedUrl}" type="application/atom+xml" />
  <author>
    <name>${config.author.name}</name>
    <uri>${config.site.url}</uri>
    <email>${config.author.email}</email>
  </author>
    $entries
</feed>